<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Progress layer -->
    <item android:id="@android:id/progress">
        <shape android:shape="rectangle">
            <solid android:color="@color/progressDefaultColor"/> <!-- Progress bar color -->
            <corners android:radius="@dimen/radius_progress" /> <!-- Rounded corners -->
        </shape>
    </item>
</layer-list>